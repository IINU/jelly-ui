{
  "name": "@getjelly/jelly-ui",
  "version": "1.1.37-alpha-1",
  "type": "module",
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsc && vite build",
    "build:types": "tsc --emitDeclarationOnly",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint-staged": "lint-staged --config ./config/lint-staged/.lintstagedrc",
    "preview": "vite preview",
    "precommit": "pnpm lint-staged",
    "test": "vitest --config ./config/vitest/vitest.config.ts run",
    "test:e2e": "pnpm exec playwright test --config config/playwright/playwright.config.ts --ui",
    "test:e2e:ci": "pnpm exec playwright test --config config/playwright/playwright.config.ts",
    "prepare": "husky",
    "build-storybook": "storybook build"
  },
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "dependencies": {
    "@tabler/icons-react": "^3.2.0",
    "@types/node": "^20.12.7",
    "date-fns": "^3.6.0",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-tooltip": "^5.26.4"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.3.3",
    "@playwright/test": "1.55.0",
    "@storybook/addon-essentials": "^8.0.8",
    "@storybook/addon-interactions": "^8.0.8",
    "@storybook/addon-links": "^8.0.8",
    "@storybook/addon-onboarding": "^8.0.8",
    "@storybook/blocks": "^8.0.8",
    "@storybook/react": "^8.0.8",
    "@storybook/react-vite": "^8.0.8",
    "@storybook/test": "^8.0.8",
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.8.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^0.8.0",
    "eslint-plugin-testing-library": "7.6.6",
    "husky": "9.1.7",
    "jest-matchmedia-mock": "^1.1.0",
    "jsdom": "26.1.0",
    "lint-staged": "16.1.6",
    "postcss": "^8.4.38",
    "prettier": "3.6.2",
    "prettier-eslint": "16.4.2",
    "storybook": "^8.0.8",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "3.2.4"
  }
}
